<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="f_devs_page.publish_time_period.listener" class="%f_devs_page.publish_time_period.listener.class%">
            <tag name="doctrine_mongodb.odm.event_listener" event="loadClassMetadata" method="loadClassMetadata" />
        </service>
        <service id="f_devs_page.publishable.listener" class="%f_devs_page.publishable.listener.class%">
            <tag name="doctrine_mongodb.odm.event_listener" event="loadClassMetadata" method="loadClassMetadata" />
        </service>
    </services>

</container>
