{% spaceless %}
    {% for prefix, prop in fields %}
        {% for key, field in prop %}
            {% if data.getDataByName(key) %}
                <meta property="{{ prefix ~ ':' ~ field.name }}" content="{{ data.getDataByName(key)|t }}"/>
            {% elseif options[key] is defined %}
                <meta property="{{ prefix ~ ':' ~ field.name }}" content="{{ options[key] }}"/>
            {% endif %}
        {% endfor %}
    {% endfor %}
{% endspaceless %}

